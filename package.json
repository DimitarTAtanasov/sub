  "main": "index.js",
  "scripts": {
    "compile": "mustache config/testnet.json nivikaSubgraph/subgraph.yaml > subgraph.yaml && graph codegen && graph build",
    "build": "tsc && node",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ greeter",
    "create-local": "graph create --node http://localhost:8020/ Greeter",
    "remove-local": "graph remove --node http://localhost:8020/ Greeter",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 Greeter",
    "test": "graph test",
    "graph-node": "docker compose --file ./nivikaSubgraph/docker-compose.yaml up -d",
    "graph-local-clean": "docker compose down -v && docker-compose rm -v && rm -rf data/ipfs data/postgres",
    "initDeployContract": "graph init deployContract --ipfs=http://localhost:5001/ipfs --abi=./abi.json --from-contract=0x4c04f449db641aafab13f852da908e9d7f12b3b5 --network=testnet --skip-git --contract-name=NivikaMarketplace  --start-block=19667617"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daniel-K-Ivanov/hedera-subgraph-repo-example.git"
