  "dependencies": {
    "@graphprotocol/graph-cli": "0.97.0",
    "@graphprotocol/graph-ts": "0.37.0"
  },
  "devDependencies": {
    "matchstick-as": "0.6.0",
    "mustache": "^4.2.0",
    "ts-node": "^10.9.2"
  }
